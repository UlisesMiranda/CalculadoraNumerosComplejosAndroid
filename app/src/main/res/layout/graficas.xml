<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Graficacion">

    <LinearLayout
        android:id="@+id/pantalla1"
        android:layout_width="411dp"
        android:layout_height="206dp"
        android:orientation="horizontal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </LinearLayout>

    <LinearLayout
        android:id="@+id/pantalla2"
        android:layout_width="409dp"
        android:layout_height="195dp"
        android:layout_marginStart="2dp"
        android:layout_marginTop="11dp"
        android:orientation="horizontal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pantalla1"></LinearLayout>

    <LinearLayout
        android:id="@+id/pantalla3"
        android:layout_width="409dp"
        android:layout_height="195dp"
        android:layout_marginTop="18dp"
        android:orientation="horizontal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pantalla2"></LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>